let processing = function(vari, num, str) {
	let info;
	info = {}
	switch (info) {
		case info==vari:
          info.variable = vari;
          console.log();
          break;
        case info==num:
          info.number = num;
          console.log();
          break;
        case info==str: 
          info.number = num;
          console.log();
          break;		  
		  
	}
}

